<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Filter" verbose="1">
    <listeners>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener" />
    </listeners>
    <test name="CorrectFilterTest">
        <classes>
            <class name="test.CorrectFilterTest">
                <methods>
                    <include name="doSelectedItemsCorrespondToFilterConditionsOfColor"/>
                    <include name="doSelectedItemsCorrespondToFilterConditionsOfSize"/>
                    <include name="doSelectedItemsCorrespondToFilterConditionsOfPriceRank"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>